             const container = document.getElementById('fContainer');

    posts.forEach(post => {
        // Create the outer slide div
        const slide = document.createElement('div');
        slide.className = 'slide';

        // Inner layout div
        slide.innerHTML = `
            <div class="d-md-flex p-3 py-lg-5 pl-lg-4 pr-lg-10">
                <span class="testimonial__quote color--coral mr-3"></span>
                <div class="hoverx3">
                    <a href="${post.link}">
                        <blockquote class="blockquote mb-3">
                            <img width="100%" style="border-radius: 10px !important;" src="${post.image}" alt="${post.title}">
                        </blockquote>
                    </a>
                    <div class="d-flex align-items-center">
                        <span class="mr-2"></span>
                        <div>
                            // <span class="badge rounded-pill bg-dark text-white p-1">Sponsored By: Microsoft</span>
                            <span class="">${post.title}</span>
                        </div>
                    </div>
                </div>
            </div>
        `;

        container.appendChild(slide); // Adds to the end
        // Use `container.prepend(slide);` if you want to add them at the top
    });
</script>


